{
    "includes": {
        "external": [
            "//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"
        ],
        "libraries": {
            "prefix": "../components",
            "items": [
                {
                    "component": "ExplorerCanvas",
                    "file": "excanvas.js",
                    "browser": "IE"
                },
                {
                    "component": "html5shiv",
                    "file": "dist/html5shiv-printshiv.js",
                    "browser": "IE <= 8"
                },
                {
                    "component": "respond",
                    "file": "respond.src.js",
                    "browser": "IE <= 8"
                },
                {
                    "component": "es5-shim",
                    "browser": "IE <= 8"
                },
                {
                    "component": "angular-bootstrap",
                    "file": "ui-bootstrap-tpls.js"
                },
                "angular-mocks",
                {
                    "component": "bootstrap",
                    "file": "dist/js/bootstrap.js"
                },
                {
                    "component": "chosen",
                    "file": ["dist/chosen.jquery.js"]
                }
            ]
        },

        "hippo-theme-plugin": {
            "prefix": "../dist",
            "items": [
                {
                    "component": "js",
                    "file": [
                        "hippo-theme.js"
                    ]
                },
                {
                    "component": "shared",
                    "file": [
                        "divider/divider.js",
                        "responsive/responsive.js",
                        "urlparser/urlparser.js",
                        "chart/chart.js",
                        "map/map.js",
                        "panel-default/panel-default.js",
                        "focus-me/focus-me.js",
                        "select-box/select-box.js"
                    ]
                }
            ]
        },

        "hippo-theme-example": {
            "items": [
                {
                    "component": "../example",
                    "file": [
                        "hippo-theme-example.js"
                    ]
                }
            ]
        }
    },

    "application": "hippo.theme.example"
}